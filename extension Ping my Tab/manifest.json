{"manifest_version": 3,
"name": "Ping my tab",
"description": "Changes your extension's icon color according to your number of tabs.", 
"version": "0.1",
"permissions": [
    "tabs"
],

"action": {
    "default_popup": "popup.html",
    "default_icon": "icons/green16.png"
}, 

"background": {
    "service_worker": "service_worker.js"
}
}
